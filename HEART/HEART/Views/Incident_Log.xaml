<Page
    x:Class="HEART.Views.Incident_Log"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:HEART.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="#FFFFFFFF">
        <StackPanel x:Name="stackCheck" Orientation="Horizontal" Margin="0,71,0,499">
            <Button x:Name="btnDaily" Content="DAY" Foreground="Black"/>
            <Button x:Name="btnWeekly" Content="WEEK" Foreground="Black"/>
            <Button x:Name="btnMonthly" Content="MONTH" Foreground="Black"/>
        </StackPanel>
        <ScrollViewer Name="scrollIncidentLog" Background="White" HorizontalAlignment="Left" Height="410" Margin="0,230,0,0" VerticalAlignment="Top" Width="400"/>
    </Grid>
</Page>
